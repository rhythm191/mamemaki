{
  "manifest_version": 3,
  "name": "mamemaki",
  "version": "0.1",
  "description": "mamemaki on chrome",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_icon": {
      "128": "images/128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["images/soy.png", "images/effect.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
